@article{lee_low_2011,
	title = {Low concentrations of honey reduce biofilm formation, quorum sensing, and virulence in \textit{{E}scherichia coli} {O}157:{H}7},
	volume = {27},
	issn = {0892-7014},
	shorttitle = {Low concentrations of honey reduce biofilm formation, quorum sensing, and virulence in {Escherichia} coli {O}157},
