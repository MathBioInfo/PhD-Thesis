	pmcid = {PMC3346669},
	keywords = {Biofilms, Anti-Bacterial Agents, Gene Expression Regulation, Bacterial, Animals, Chemical Fractionation, Disulfides, Drug Synergism, Garlic, Genes, Reporter, Glycolipids, Mice, Neutrophils, Plant Extracts, Pseudomonas aeruginosa, Pseudomonas Infections, Quorum Sensing, Respiratory Tract Infections, Tobramycin, Virulence Factors},
